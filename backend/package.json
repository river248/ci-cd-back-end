{
    "name": "ci-cd-back-end",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "engines": {
        "node": "18.16.0"
    },
    "scripts": {
        "test": "jest --ci --maxWorkers=50% --silent --coverage --coverageReporters=json-summary --coverageDirectory=. --passWithNoTests --json --outputFile=junit.json",
        "eslint": "eslint src",
        "clean": "rm -rf build && mkdir build",
        "build-babel": "babel ./src -d ./build/src",
        "build": "npm run clean && npm run build-babel",
        "production": "npm run build && node ./build/src/server.js",
        "dev": "nodemon --exec ./node_modules/.bin/babel-node ./src/server.js",
        "dbmigration": "nodemon --exec ./node_modules/.bin/babel-node ./src/dbmigration",
        "start": "node ./build/src/server.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "jest": {
        "reporters": [
            "default",
            "jest-junit",
            "jest-html-reporters"
        ],
        "collectCoverageFrom": [
            "**/*.{js,jsx}",
            "!**/node_modules/**",
            "!**/coverage/**"
        ]
    },
    "devDependencies": {
        "@babel/cli": "^7.21.5",
        "@babel/core": "^7.22.1",
        "@babel/node": "^7.22.1",
        "@babel/plugin-transform-runtime": "^7.22.4",
        "@babel/preset-env": "^7.22.4",
        "babel-plugin-module-resolver": "^5.0.0",
        "jest": "^29.5.0",
        "jest-html-reporters": "^3.1.4",
        "jest-junit": "^16.0.0",
        "nodemon": "^2.0.22"
    },
    "dependencies": {
        "@babel/runtime": "^7.22.3",
        "axios": "^1.4.0",
        "cors": "^2.8.5",
        "dotenv": "^16.1.3",
        "express": "^4.18.2",
        "firebase-admin": "^11.9.0",
        "joi": "^17.9.2",
        "lodash": "^4.17.21",
        "mongodb": "^5.5.0",
        "octokit": "^2.0.19",
        "socket.io": "^4.6.2"
    }
}
